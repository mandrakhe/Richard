@model PROYECTO_NET.Data.Skill

@{
    ViewData["Title"] = "Delete";
}


<h3>Estás seguro de que quieres eliminarla?</h3>
<div>
    <h4>Competencias</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NameSkill)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NameSkill)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Description)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Atrás</a>
    </form>
</div>
